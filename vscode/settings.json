{
  // VIM
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "="],
      "after": ["<C-a>"]
    },
    {
      "before": ["<leader>", "-"],
      "after": ["<C-x>"]
    },
    {
      "before": ["<leader>", "o"],
      "after": ["o", "<ESC>", "k", "_"]
    },
    {
      "before": ["<leader>", "O"],
      "after": ["O", "<ESC>", "j", "_"]
    },
    {
      "before": ["<leader>", "<ESC>"],
      "commands": [":nohl"],
      "slient": true
    },
    {
      "before": ["<leader>", "<leader>", "p"],
      "commands": [
        "editor.action.clipboardPasteAction",
        "editor.action.reindentselectedlines"
      ]
    },
    {
      "before": ["<leader>", "<leader>", "w"],
      "commands": ["workbench.action.files.saveWithoutFormatting"]
    },
    {
      "before": ["<leader>", "p"],
      "after": ["\"", "_", "d", "P"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["<C-d>"],
      "after": ["<C-d>", "z", "z"]
    },
    {
      "before": ["<C-u>"],
      "after": ["<C-u>", "z", "z"]
    },
    {
      "before": ["G"],
      "after": ["G", "z", "z"]
    },
    {
      "before": ["n"],
      "after": ["n", "z", "z", "z", "v"]
    },
    {
      "before": ["N"],
      "after": ["N", "z", "z", "z", "v"]
    },
    {
      "before": ["<leader>", "w", "l"],
      "after": ["<C-w>", "v"]
    },
    {
      "before": ["<leader>", "w", "j"],
      "after": ["<C-w>", "s"]
    },
    {
      "before": ["<leader>", "w", "e"],
      "after": ["<C-w>", "="]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": ["workbench.action.toggleAuxiliaryBar"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    },
    {
      "before": ["<leader>", "<leader>", "t"],
      "commands": ["workbench.action.terminal.openNativeConsole"]
    },
    {
      "before": ["<leader>", "s", "s"],
      "commands": ["search.action.openNewEditorFromView"]
    },
    {
      "before": ["<leader>", "f", "s"],
      "commands": ["actions.find"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.view.search"]
    },
    {
      "before": ["<leader>", "f", "p"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "f", "o"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["[", "g"],
      "commands": ["workbench.action.editor.nextChange"]
    },
    {
      "before": ["]", "g"],
      "commands": ["workbench.action.editor.previousChange"]
    },
    {
      "before": ["<leader>", "g", "a"],
      "commands": ["git.clean"]
    },
    {
      "before": ["<leader>", "g", "f"],
      "commands": ["git.openChange"]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": ["gitlens.toggleLineBlame"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["vscode-harpoon.addEditor"]
    },
    {
      "before": ["<leader>", "<Tab>"],
      "commands": ["vscode-harpoon.editEditors"]
    },
    {
      "before": ["<leader>", "<leader>", "<Tab>"],
      "commands": ["vscode-harpoon.editorQuickPick"]
    },
    {
      "before": ["<leader>", "1"],
      "commands": ["vscode-harpoon.gotoEditor1"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["vscode-harpoon.gotoEditor2"]
    },
    {
      "before": ["<leader>", "3"],
      "commands": ["vscode-harpoon.gotoEditor3"]
    },
    {
      "before": ["<leader>", "4"],
      "commands": ["vscode-harpoon.gotoEditor4"]
    },
    {
      "before": ["<leader>", "5"],
      "commands": ["vscode-harpoon.gotoEditor5"]
    },
    {
      "before": ["<leader>", "6"],
      "commands": ["vscode-harpoon.gotoEditor6"]
    },
    {
      "before": ["<leader>", "d", "e"],
      "commands": ["workbench.debug.action.toggleRepl"]
    },
    {
      "before": ["<leader>", "d", "d"],
      "commands": ["editor.debug.action.toggleBreakpoint"]
    },
    {
      "before": ["<leader>", "d", "r"],
      "commands": ["workbench.action.debug.start"]
    },
    {
      "before": ["<leader>", "d", "y"],
      "commands": ["workbench.action.debug.stop"]
    },
    {
      "before": ["<leader>", "d", "u"],
      "commands": ["workbench.action.debug.continue"]
    },
    {
      "before": ["<leader>", "d", "j"],
      "commands": ["workbench.action.debug.stepOver"]
    },
    {
      "before": ["<leader>", "d", "i"],
      "commands": ["workbench.action.debug.stepInto"]
    },
    {
      "before": ["<leader>", "d", "o"],
      "commands": ["workbench.action.debug.stepOut"]
    },
    {
      "before": ["<leader>", "d", "h"],
      "commands": ["workbench.action.debug.restart"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<leader>", "p"],
      "after": ["\"", "_", "d", "P"]
    },
    {
      "before": ["<leader>", "p"],
      "after": ["\"", "_", "d", "P"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": ["n"],
      "commands": ["editor.action.addSelectionToNextFindMatch"]
    },
    {
      "before": ["N"],
      "commands": ["editor.action.addSelectionToPreviousFindMatch"]
    },
    {
      "before": ["*"],
      "after": ["y", "/", "\\V", "<C-R>", "\"", "<CR>", "<Esc>"]
    },
    {
      "before": ["<leader>", "<C-e>"],
      "commands": ["github.copilot.edits.attachSelection"]
    },
    {
      "before": ["<leader>", "<C-c>"],
      "commands": ["github.copilot.chat.attachSelection"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["<C-p>"],
      "commands": ["editor.action.clipboardPasteAction"]
    }
  ],
  "vim.hlsearch": true,
  "vim.ignorecase": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(255, 140, 85, 1.0)",
  "vim.highlightedyank.textColor": "black",
  "vim.highlightedyank.duration": 150,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-o>": false,
    "<C-i>": false
  },
  "vim.commandLineModeKeyBindings": [
    {
      "before": ["W"],
      "after": ["w"]
    }
  ],
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1,
    "bradlc.vscode-tailwindcss": 3,
    "vue.volar": 2,
    "vscode.typescript-language-features": 2,
    "typescriptteam.native-preview": 4,
    "dbaeumer.vscode-eslint": 4
  },
  // VIM
  // LANGUAGES
  "vue.server.includeLanguages": ["vue"],
  "vue.format.style.initialIndent": false,
  "svelte.enable-ts-plugin": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "svelte"
  ],
  "typescript.experimental.useTsgo": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[javascript][javascriptreact][typescript][typescriptreact][vue][svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "quickfix.insertSemicolon": "explicit"
    },
    "editor.selectionHighlight": false
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    },
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced"
  },
  "json.format.enable": false,
  "json.validate.enable": true,
  "[json][jsonc][jsonl]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "rust-analyzer.diagnostics.disabled": ["unresolved-proc-macro"],
  "go.toolsManagement.autoUpdate": true,
  "gopls": {
    "ui.navigation.importShortcut": "Link"
  },
  "emmet.includeLanguages": {
    "rust": "html",
    "*.rs": "html"
  },
  "docker.extension.enableComposeLanguageServer": false,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "makefile.configureOnOpen": false,
  // LANGUAGES
  // UTILS
  "gitlens.graph.layout": "editor",
  "githubPullRequests.pullBranch": "never",
  "github.copilot.chat.codeGeneration.useInstructionFiles": true,
  "github.copilot.chat.agent.autoFix": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.useResponsesApi": true,
  "github.copilot.chat.responsesApiReasoningEffort": "high",
  "github.copilot.nextEditSuggestions.extendedRange": true,
  "github.copilot.chat.tools.memory.enabled": false,
  "github.copilot.enable": {
    "secret": false,
    "appsettings_secret": false
  },
  "files.associations": {
    "*.rmd": "markdown",
    "*.rs": "rust",
    "**/appsettings.*.json": "appsettings_secret",
    "**/.env*": "secret"
  },
  "chat.viewSessions.orientation": "stacked",
  "chat.agent.maxRequests": 100,
  "chat.tools.global.autoApprove": true,
  "chat.tools.terminal.autoApprove": {
    "/^pnpm tsc --noEmit 2>&1 \\| head -100$/": {
      "approve": true,
      "matchCommandLine": true
    }
  },
  // UTILS
  // EDITOR
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font', 'Fira Code', 'Cascadia Code PL', 'CaskaydiaCove Nerd Font', Consolas, 'Courier New', monospace",
  "terminal.integrated.allowedLinkSchemes": [
    "file",
    "http",
    "https",
    "mailto",
    "vscode",
    "vscode-insiders",
    "docker-desktop"
  ],
  "terminal.integrated.commandsToSkipShell": ["workbench.action.togglePanel"],
  "terminal.integrated.allowChords": false,
  "window.zoomLevel": 0.1,
  "window.titleBarStyle": "custom",
  "explorer.openEditors.visible": 4,
  "explorer.compactFolders": false,
  "editor.accessibilitySupport": "off",
  "editor.formatOnSave": true,
  "editor.fontSize": 14,
  "editor.fontFamily": "'FiraCode Nerd Font', 'Fira Code', 'Cascadia Code PL', 'CaskaydiaCove Nerd Font', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.stickyScroll.enabled": true,
  "editor.wordWrap": "on",
  "editor.rulers": [120],
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "none",
  "editor.guides.bracketPairs": true,
  "editor.lineNumbers": "relative",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "prettier.printWidth": 120,
  "prettier.tabWidth": 2,
  "prettier.endOfLine": "auto",
  "prettier.singleQuote": true,
  "prettier.semi": true,
  "prettier.useTabs": false,
  "prettier.trailingComma": "all",
  "prettier.experimentalTernaries": true,
  "workbench.colorTheme": "Dracula Pro",
  "workbench.editor.showTabs": "none",
  "workbench.editor.revealIfOpen": true,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.enablePreview": false,
  "workbench.activityBar.location": "hidden",
  "workbench.editorAssociations": { "*.svg": "default" },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {
    "[Dracula Pro]": {
      "editorBracketPairGuide.activeBackground1": "#FFD700",
      "editorBracketPairGuide.activeBackground2": "#DA70D6",
      "editorBracketPairGuide.activeBackground3": "#179FFF",
      "editorBracketPairGuide.activeBackground4": "#FFD700",
      "editorBracketPairGuide.activeBackground5": "#DA70D6",
      "editorBracketPairGuide.activeBackground6": "#179FFF",
      "editorBracketPairGuide.background1": "#FFFFFF00",
      "editorBracketPairGuide.background2": "#FFFFFF00",
      "editorBracketPairGuide.background3": "#FFFFFF00",
      "editorBracketPairGuide.background4": "#FFFFFF00",
      "editorBracketPairGuide.background5": "#FFFFFF00",
      "editorBracketPairGuide.background6": "#FFFFFF00",
      "editorBracketHighlight.foreground1": "#FFD700",
      "editorBracketHighlight.foreground2": "#DA70D6",
      "editorBracketHighlight.foreground3": "#179FFF",
      "editorBracketHighlight.foreground4": "#FFD700",
      "editorBracketHighlight.foreground5": "#DA70D6",
      "editorBracketHighlight.foreground6": "#179FFF"
    }
  },
  // EDITOR
  // MATERIAL ICONS FOLDER
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.folders.associations": {
    "infra": "app",
    "entities": "class",
    "schemas": "class",
    "typeorm": "database",
    "repositories": "mappings",
    "http": "container",
    "migrations": "tools",
    "modules": "components",
    "implementations": "core",
    "dtos": "typescript",
    "fakes": "mock",
    "enums": "scala",
    "providers": "Hook",
    "interfaces": "Class",
    "atoms": "Redux-selector",
    "molecules": "Redux-reducer",
    "organisms": "Redux-store",
    "templates": "Redux-actions",
    "stores": "Redux-store",
    "maps": "Target",
    "features": "Template"
  },
  "material-icon-theme.folders.theme": "specific",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
    "pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata",
    "*.dart": "${capture}.g.dart",
    "Cargo.toml": "Cargo.lock",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  }
  // MATERIAL ICONS FOLDER
}
