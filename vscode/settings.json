        },
        {
            "before": [
                "<leader>",
                "f",
                "s"
            ],
            "commands": [
                "actions.find"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "p"
            ],
            "commands": [
                "workbench.action.quickOpen"
