{
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "+"
            ],
            "after": [
                "<C-a>"
            ]
        },
        {
            "before": [
                "<leader>",
                "-"
            ],
            "after": [
                "<C-x>"
            ]
        },
        {
            "before": [
                "x"
            ],
            "after": [
                "_",
                "x"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "P"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "J"
